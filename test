     //    for (int j = 0; j < 11 - i - 1; j++) {
      //      if (arr[j] > arr[j + 1]) {
       //         temp = arr[j];
        //        arr[j] = arr[j + 1];
       //         arr[j + 1] = temp;
        //    }
       // }
   // }
Serial.print(mass[0]);
Serial.println(" cm"); 
delay(1000);
Serial.print(mass[3]);
Serial.println(" cm1"); 
delay(1000);
Serial.print(mass[10]);
Serial.println(" cm2");     
